
{
  "manifest_version": 3,
  "name": "The OSR Webring",
  "permissions": [
    "storage",
    "tabs",
    "sidePanel"
  ],
  "description": "OSR Webring",
  "icons": {
    "16": "icons/osr-48.png",
    "48": "icons/osr-48.png",
    "128": "icons/osr-48.png"
  },
  "version": "1.0.3",
  "action": {
    "default_popup": "sidebar/osr.html",
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "sidebar/osr.html"
  }
}