{

  "manifest_version": 2,
  "name": "The OSR Webring",
  "version": "1.0",

  "browser_specific_settings": {
    "gecko": {
      "id": "@totalityofygg.com",
      "strict_min_version" : "54.0a1"
    }
  },

  "description" : "OSR Webring",

  "icons": {
    "48": "icons/border-48.png"
  },

  "sidebar_action": {
    "default_title": "The OSR Webring",
    "default_panel": "sidebar/osr.html",
    "open_at_install" : true
  },

  "permissions": ["storage", "tabs"]

}

